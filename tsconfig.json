{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "composite": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "declaration": true,
    "noImplicitAny": true,
    "paths": {
      "*": [
        "*",
        "./*",
        "./bazel-out/k8-fastbuild/bin/*",
        "./bazel-out/k8-fastbuild/bin/components/*",
        "./bazel-out/k8-dbg/bin/*",
        "./bazel-out/darwin-fastbuild/bin/*",
        "./bazel-out/darwin-fastbuild/bin/*",
        "./bazel-out/darwin-dbg/bin/*",
        "./bazel-out/x64_windows-fastbuild/bin/*",
        "./bazel-out/x64_windows-dbg/bin/*"
      ]
    },
    "rootDirs": [
      ".",
      "./bazel-out/k8-fastbuild/bin",
      "./bazel-out/k8-fastbuild/bin/components",
      "./bazel-out/k8-dbg/bin",
      "./bazel-out/darwin-fastbuild/bin",
      "./bazel-out/x64_windows-fastbuild/bin",
      "./bazel-out/x64_windows-dbg/bin"
    ],
    "noEmit": true,
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.css"
  ],
  "exclude": [
    "node_modules"
  ]
}
